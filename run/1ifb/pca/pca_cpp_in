parm ../../../top_files/dry_1ifb.prmtop
trajin ../data/full_fit_1ifb.nc
rms first :7-126@CA
average avg_1ifb.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference avg_1ifb.pdb [avg]
rms ref [avg] :7-126@CA RMSD
matrix mwcovar name mtx_1ifb :7-126@CA
createcrd crd_1ifb
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_1ifb vecs 354 out modes_1ifb
go
