parm ../../../top_files/dry_4uet.prmtop
trajin ../data/full_fit_4uet.nc
rms first :10-144@CA
average full_avg_4uet.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_4uet.pdb [avg]
rms ref [avg] :10-144@CA RMSD
matrix mwcovar name mtx_4uet :10-144@CA
createcrd crd_4uet
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_4uet vecs 399 out full_modes_4uet
go
