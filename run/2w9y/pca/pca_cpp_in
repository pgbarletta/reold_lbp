parm ../../../top_files/dry_2w9y.prmtop
trajin ../data/full_fit_2w9y.nc
rms first :7-132@CA
average full_avg_2w9y.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_2w9y.pdb [avg]
rms ref [avg] :7-132@CA RMSD
matrix mwcovar name mtx_2w9y :7-132@CA
createcrd crd_2w9y
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_2w9y vecs 372 out full_modes_2w9y
go
