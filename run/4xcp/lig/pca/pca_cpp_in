parm ../../../../top_files/dry_4xcp.prmtop
trajin ../../data/full_fit_4xcp.nc
rms first @2427-2444
average full_avg_lig_4xcp.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_lig_4xcp.pdb [avg]
rms ref [avg] @2427-2444 RMSD
matrix mwcovar name mtx_4xcp @2427-2444
createcrd crd_4xcp
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_4xcp vecs 48 out full_modes_lig_4xcp
go
