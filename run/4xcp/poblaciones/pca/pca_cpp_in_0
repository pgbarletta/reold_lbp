parm ../../../../top_files/dry_4xcp.prmtop
trajin ../vol0_4xcp.nc
rms first :10-144@CA
average full_avg_4xcp_0.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_4xcp_0.pdb [avg]
rms ref [avg] :10-144@CA RMSD
matrix mwcovar name mtx_4xcp :10-144@CA
createcrd crd_4xcp
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_4xcp vecs 399 out full_modes_4xcp_0
go
