parm ../../../top_files/dry_2ifb.prmtop
trajin ../data/full_fit_2ifb.nc
rms first :7-126@CA
average avg_2ifb.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference avg_2ifb.pdb [avg]
rms ref [avg] :7-126@CA RMSD
matrix mwcovar name mtx_2ifb :7-126@CA
createcrd crd_2ifb
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_2ifb vecs 354 out modes_2ifb
go
