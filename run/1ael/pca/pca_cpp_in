parm ../../../top_files/dry_1ael.prmtop
trajin ../data/full_fit_1ael.nc
rms first :7-126@CA
average full_avg_1ael.pdb :7-126@CA pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_1ael.pdb [avg]
rms ref [avg] :7-126@CA RMSD
matrix mwcovar name mtx_1ael :7-126@CA
createcrd crd_1ael
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_1ael vecs 354 out full_modes_1ael
go
