source leaprc.protein.ff14SB
source leaprc.water.tip3p
source leaprc.gaff

# Load ligand  parameters
loadoff lig/PLM.lib
# Load complex
cpx = loadpdb h1ure.pdb
saveamberparm cpx dry_1ure.prmtop dry_1ure.rst7

charge cpx
addions2 cpx Cl- 0
addions2 cpx Na+ 0
solvateoct cpx TIP3PBOX 10
saveamberparm cpx 1ure.prmtop 1ure.rst7

quit
