source leaprc.protein.ff14SB
source leaprc.water.tip3p
source leaprc.gaff

# Load ligand  parameters
loadoff lig/PLM.lib
# Load complex
cpx = loadpdb h2ifb.pdb
saveamberparm cpx dry_2ifb.prmtop dry_2ifb.rst7

charge cpx
addions2 cpx Cl- 0
addions2 cpx Na+ 0
solvateoct cpx TIP3PBOX 10
saveamberparm cpx 2ifb.prmtop 2ifb.rst7

quit
